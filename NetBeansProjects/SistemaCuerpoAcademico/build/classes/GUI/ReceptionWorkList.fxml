<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="GUI.ReceptionWorkListController">
    <stylesheets>
        <URL value="@receptionworklist.css" />
    </stylesheets>
    <children>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#202122" height="31.0" layoutY="-1.0" stroke="BLACK" strokeType="INSIDE" width="600.0" />
      <Button fx:id="btRegisterReceptionWork" layoutX="455.0" layoutY="39.0" mnemonicParsing="false" onAction="#actionRegisterReceptionWork" text="Registrar Trabajo " />
      <ListView fx:id="lvReceptionWorks" layoutX="65.0" layoutY="124.0" prefHeight="200.0" prefWidth="470.0" />
      <Label layoutX="65.0" layoutY="90.0" text="Selecciona el trabajo recepcional a consultar:">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Button fx:id="btReturn" layoutX="510.0" layoutY="361.0" mnemonicParsing="false" onAction="#actionReturn" text="Regresar" />
     </children>
</AnchorPane>
